{
  "expo": {
    "name": "kuairongbei",
    "slug": "kuairongbei",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "kuairongbei",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.kuairongbei.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-barcode-scanner",
        {
          "cameraPermission": "允许访问相机以扫描二维码"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "允许访问相机以拍照和录像",
          "microphonePermission": "允许访问麦克风以录制视频"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "允许访问相册以选择图片和视频"
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "允许访问相册以保存和读取媒体文件",
          "savePhotosPermission": "允许保存图片到相册",
          "isAccessMediaLocationEnabled": true
        }
      ],
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "479692e2-a986-40ec-bbfb-dbee6ef7b3cc"
      }
    }
  }
}
